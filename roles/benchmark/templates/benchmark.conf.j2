[program:muta_benchmark]
command=node index -d {{ duration }} -g {{ gap }} -c {{ connections }} --cpu {{ cpu }} --pk {{ pk }} --json benchmark_nodes
directory=benchmark_dir
numprocs=1
autostart=true
autorestart=true
startretries=10
exitcodes=0
stopasgroup=true
killasgroup=true
stopsignal=KILL
stopwaitsecs=10
redirect_stderr=true
stdout_logfile=/var/log/muta-benchmark.log
stdout_logfile_maxbytes=30MB
